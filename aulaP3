def leitura(v):
for i in range(5):
aux = int(input("Valor: "))
v.append(aux)



def escrita(v):
print("[", end =" ")
for i in range(len(v)):
print(v[i], end=" ")
print("]")



def merge(v1,v2,v3):
for i in range(5):
v3.append(v1[i])
v3.append(v2[i])



v1 = []
v2 = []
v3 = []


leitura(v1)
escrita(v1)


leitura(v2)
escrita(v2)


merge(v1,v2,v3)


escrita(v3)
